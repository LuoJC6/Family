<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kakaka.MainActivity" >

		<TableRow android:id="@+id/TR1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		    
		    <TextView android:id="@+id/tv1"
		        android:layout_width="50dp"
		        android:layout_height="wrap_content"
		        android:textSize="24px"
		        android:text="收益率"
		        android:gravity="center"/>
		    <TextView android:id="@+id/tv2"
		        android:layout_width="65dp"
		        android:layout_height="wrap_content"
		        android:textSize="24px"
		        android:text="存取时间"
		        android:gravity="center"/>
		    <TextView android:id="@+id/tv3"
		        android:layout_width="65dp"
		        android:layout_height="wrap_content"
		        android:textSize="24px"
		        android:text="起购金额"
		        android:gravity="center"/>
		    <TextView android:id="@+id/tv4"
		        android:layout_width="80dp"
		        android:layout_height="wrap_content"
		        android:textSize="24px"
		        android:text="产品名称"
		        android:gravity="center"/>
		    <TextView android:id="@+id/tv5"
		        android:layout_width="65dp"
		        android:layout_height="wrap_content"
		        android:textSize="24px"
		        android:text="状态"
		        android:gravity="center"/>
		</TableRow>
		<TableRow android:id="@+id/TR2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">
		    
		    <TextView android:id="@+id/tv6"
		        android:layout_width="50dp"
		        android:layout_height="wrap_content"
		        android:textSize="24px"
		        android:text="3.93%"
		        android:gravity="center"/>
		    <TextView android:id="@+id/tv7"
		        android:layout_width="65dp"
		        android:layout_height="wrap_content"
		        android:textSize="24px"
		        android:text="随时存取"
		        android:gravity="center"/>
		    <TextView android:id="@+id/tv8"
		        android:layout_width="65dp"
		        android:layout_height="wrap_content"
		        android:textSize="24px"
		        android:text="100元起购"
		        android:gravity="center"/>
		    <TextView android:id="@+id/tv9"
		        android:layout_width="80dp"
		        android:layout_height="wrap_content"
		        android:textSize="24px"
		        android:text="平安养老富盈5号"
		        android:gravity="center"/>
		    <Button android:id="@+id/btn2"
		        android:layout_width="50dp"
		        android:layout_height="wrap_content"
		        android:textSize="20px"
		        android:text="立即抢购"
		        android:gravity="center"/>
		</TableRow>
